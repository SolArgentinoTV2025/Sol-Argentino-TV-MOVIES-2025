{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:page",
      "destination": "/:page.html",
      "has": [
        {
          "type": "file",
          "value": "/:page.html"
        }
      ]
    },
    {
      "source": "/:folder",
      "destination": "/:folder/:folder.html",
      "has": [
        {
          "type": "file",
          "value": "/:folder/:folder.html"
        }
      ]
    }
  ]
}
