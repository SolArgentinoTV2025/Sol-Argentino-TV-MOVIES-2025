{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:folder/:folder",
      "destination": "/:folder/:folder.html"
    },
    {
      "source": "/:page",
      "destination": "/:page.html"
    }
  ]
}
